{
  "name": "tradezap",
  "description": "CLI tool for downloading trading data",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "README.md"
  ],
  "bin": {
    "tradezap": "dist/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf dist && bun run build.ts",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.json",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "test": "bun test",
    "test-watch": "bun test --watch",
    "test-coverage": "bun test --coverage",
    "semantic-release": "semantic-release --branches main",
    "tradezap-latest": "bunx tradezap --config tradezap.ts",
    "clean": "rm -rf node_modules coverage dist compiled"
  },
  "dependencies": {
    "cac": "6.7.14",
    "chalk": "5.3.0"
  },
  "devDependencies": {
    "bun-types": "1.0.1",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "semantic-release": "21.1.1",
    "typescript": "5.2.2"
  },
  "license": "MIT",
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "homepage": "https://github.com/ocignis/tradezap",
  "repository": "ocignis/tradezap",
  "bugs": "https://github.com/ocignis/tradezap/issues",
  "keywords": [
    "trade-data",
    "trading",
    "kline",
    "tradezap",
    "cli",
    "cli-tool"
  ]
}
